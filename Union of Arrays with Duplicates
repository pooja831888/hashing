class Solution {
    public static int findUnion(int a[], int b[]) {
        // code here
        HashMap<Integer,Integer> map=new HashMap<>();
        
        for(int num:a)
        {
            map.put(num,1);
        }
        
         for(int num:b)
        {
            map.put(num,1);
        }
        
        return map.size();
        
        
    }
}
